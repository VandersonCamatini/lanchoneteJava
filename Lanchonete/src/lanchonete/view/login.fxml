<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lanchonete.controller.loginController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="300.0">
         <children>
            <TextField fx:id="tfUsername" layoutX="76.0" layoutY="84.0" prefHeight="25.0" prefWidth="171.0" promptText="Nome de usuário" />
            <PasswordField fx:id="pfPassword" layoutX="76.0" layoutY="125.0" prefHeight="25.0" prefWidth="171.0" promptText="Senha do usuário" />
            <CheckBox fx:id="cbGerente" layoutX="75.0" layoutY="179.0" mnemonicParsing="false" onAction="#cbGerenteOnAction" text="Gerente" />
            <CheckBox fx:id="cbVendedor" layoutX="173.0" layoutY="179.0" mnemonicParsing="false" onAction="#cbVendedorOnAction" text="Vendedor" />
            <Button fx:id="btnEnter" layoutX="72.0" layoutY="228.0" mnemonicParsing="false" onAction="#btnEnterOnAction" prefHeight="25.0" prefWidth="70.0" text="Entrar" />
            <Button fx:id="btnExit" layoutX="175.0" layoutY="228.0" mnemonicParsing="false" onAction="#btnExitOnAction" prefHeight="25.0" prefWidth="70.0" text="Sair" />
         </children>
         <padding>
            <Insets bottom="-160.0" />
         </padding>
      </AnchorPane>
   </children>
</AnchorPane>
